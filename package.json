{
  "name": "readbook",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js",
    "serve:api": "npm run update-version && npm run cp-all-files && nx serve api --configuration=dev",
    "serve:bo": "npm run update-version && nx serve app --port 4200 --host 0.0.0.0 --open --configuration=dev",
    "build": "nx build",
    "test": "nx test",
    "update-version": "grep $npm_package_version version.json || (npm run change-version)",
    "change-version": "node -e \"console.log('{ \\\"WARNING\\\":\\\"AUTOGENERATED FILE, DO NOT MODIFY\\\", \\\"VERSION\\\": \\\"' + process.env.npm_package_version +'\\\", \\\"NAME\\\": \\\"' + process.env.npm_package_name +'\\\" }')\" >",
    "// COPY FILES SCRIPTS //": "",
    "cp-all-files": "npm run cp-patcher && npm run cp-soap-wsdl",
    "cp-patcher": "[ ! -d patches ] || (mkdir -p dist/apps/api/patches && cp -r patches/* dist/apps/api/patches/)",
    "cp-soap-wsdl": "[ ! -d apps/api/src/app/shared/soap/wsdl ] || (mkdir -p dist/apps/api/wsdl && cp -r apps/api/src/app/shared/soap/wsdl/* dist/apps/api/wsdl/)"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.2.0",
    "@angular/common": "~14.2.0",
    "@angular/compiler": "~14.2.0",
    "@angular/core": "~14.2.0",
    "@angular/forms": "~14.2.0",
    "@angular/platform-browser": "~14.2.0",
    "@angular/platform-browser-dynamic": "~14.2.0",
    "@angular/router": "~14.2.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.1.3",
    "@nestjs/typeorm": "^9.0.1",
    "@nguniversal/express-engine": "^14.2.1",
    "@nrwl/angular": "^14.7.5",
    "@nrwl/webpack": "^15.0.10",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "mysql2": "^2.3.3",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "tslib": "^2.3.0",
    "typeorm": "^0.3.10",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.2.0",
    "@angular-eslint/eslint-plugin": "~14.0.4",
    "@angular-eslint/eslint-plugin-template": "~14.0.4",
    "@angular-eslint/template-parser": "~14.0.4",
    "@angular/cli": "~14.2.0",
    "@angular/compiler-cli": "~14.2.0",
    "@angular/language-service": "~14.2.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nrwl/cli": "15.0.10",
    "@nrwl/cypress": "15.0.10",
    "@nrwl/eslint-plugin-nx": "15.0.10",
    "@nrwl/jest": "15.0.10",
    "@nrwl/linter": "15.0.10",
    "@nrwl/nest": "^14.7.5",
    "@nrwl/node": "^14.7.5",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/workspace": "15.0.10",
    "@types/jest": "28.1.1",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "cypress": "^10.7.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "jest-preset-angular": "~12.2.2",
    "nx": "15.0.10",
    "prettier": "^2.6.2",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2"
  }
}
